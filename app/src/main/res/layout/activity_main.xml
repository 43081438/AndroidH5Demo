<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    tools:context="com.android.qzy.androidandh5demo.MainActivity">

    <Button
        android:id="@+id/btn_base_load"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="WebView加载"/>

    <Button
        android:id="@+id/btn_native"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="本地H5调用简单实现"/>

    <Button
        android:id="@+id/btnAssemble"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="本地H5调用封装实现"/>
</LinearLayout>
